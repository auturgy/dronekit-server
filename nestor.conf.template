include "application"

# This is the new 3dr specific keys for dronehub
dapi {
  # if threescale keys are not set threescale will not be used for API control
  threescale {
    # apiKey = "FIXME"
    # serviceId = "FIXME"
  }

  aws {
    # 3dr keys
    accessKey = ${AWS_ACCESS_KEY}
    secretKey = ${AWS_SECRET_KEY}
  }

  s3 {
    # default value
    bucketName = "s3-droneapi"
    # optional environment override
    bucketName = ${?S3_BUCKET_NAME}
  }

  # if you don't want server to pick a random root password set this here
  # defaultRootPsw="FIXME"
}
